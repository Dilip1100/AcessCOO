SELECT MDG.MATNR, MDG.WERKS, MARA.ZSUPPSITE, MARA.MTART, MAKT.MAKTX, T.Old, FR.HERKL, EU.HERKL, ASIA.HERKL, NA.HERKL, ICON.HERKL, LA.HERKL
FROM ((((((((MDG_MARC AS MDG LEFT JOIN MDG_MARA AS MARA ON MDG.MATNR = MARA.MATNR) LEFT JOIN MDG_MAKT AS MAKT ON MDG.MATNR = MAKT.MATNR) LEFT JOIN TGL AS T ON (MDG.WERKS = T.Plant) AND (MDG.MATNR = T.NEWMaterial)) LEFT JOIN FR_ERP AS FR ON (MDG.WERKS = FR.WERKS) AND (MDG.MATNR = FR.MATNR)) LEFT JOIN EU_ERP AS EU ON (MDG.WERKS = EU.WERKS) AND (MDG.MATNR = EU.MATNR)) LEFT JOIN ASIA_ERP AS ASIA ON (MDG.WERKS = ASIA.WERKS) AND (MDG.MATNR = ASIA.MATNR)) LEFT JOIN NAM_ERP AS NA ON (MDG.WERKS = NA.WERKS) AND (MDG.MATNR = NA.MATNR)) LEFT JOIN ICON_ERP AS ICON ON (T.Plant = ICON.WERKS) AND (T.Old = ICON.MATNR)) LEFT JOIN LATAM_ERP AS LA ON (T.Plant = LA.WERKS) AND (T.Old = LA.MATNR)
WHERE (((MDG.WERKS)<>"DE01"));
